<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Project List</title>

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Be+Vietnam+Pro:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet">

    <link rel="stylesheet" href="/CSS/reset.css">
    <link rel="stylesheet" href="/CSS/variables.css">
    <link rel="stylesheet" href="/CSS/style.css">
    
</head>
<body>

    <div class="search-data-row" id="table-header">
        <div>Project Number</div>
            <div>Project Name</div>
            <div>Start of Production</div>
            <div>Platform</div>
            <div>Model</div>
            <div>Vehicle Model Year</div>
        
    </div>
    <% searchData.forEach(item => { %>
        <div class="search-data-row">
            <div> <a href='/project/<%= item.projectnumber %>'> <%= item.projectnumber %> </a> </div>
            <div> <%= item.projectname %></div>
            <div> <%= item.startofproduction %></div>
            <div> <%= item.projectplatform %></div>
            <div> <%= item.projectmodel %></div>
            <div> <%= item.projectmodelyear %></div>
        </div>
    <% }) %>
</body>
</html>